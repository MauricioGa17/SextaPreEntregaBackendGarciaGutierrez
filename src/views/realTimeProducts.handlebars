<div>
    <label>Saludos</label>

    <div>
        <h4>Lista</h4>

        <form id="form-products">
            <input type="text" id="title" placeholder="title" required />
            <input type="text" id="descripcion" placeholder="Description" required />
            <input type="text" id="code" placeholder="code" required />
            <input type="number" step="0.01" id="price" placeholder="0.00$" required />
            <input type="text" id="status" placeholder="status" required />
            <input type="number" id="stock" placeholder="stock" required />
            <input type="text" id="category" placeholder="category" required />
            <input type="text" id="thumbnails" placeholder="thumbnails" required />
            <button type="submit">Registrar Producto</button>
        </form>

        <hr>

        <table>
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Opcion</th>
                </tr>
            </thead>
            <tbody id="products">
            </tbody>
        </table>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/static/js/realTimeProducts.js"></script>
</div>